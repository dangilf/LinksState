
@{
    ViewBag.Title = "ViewReportResult";
}

<h2>ViewReportResult</h2>

<div id="app">
    <p v-if="!personal || results.length == 0">
        <button v-on:click="startCheck">Start Check</button>
    </p>
    <table v-if="results.length > 0" class="table table-bordered">
        <thead>
            <tr>
                <th>Url</th>
                <th>Status Code</th>
            </tr>
        </thead>
        <tbody>
            <tr v-for="result in results">
                <td style="max-width:400px; word-wrap:break-word">{{result.URL}}</td>
                <td>{{result.StatusCode}}</td>
            </tr>
        </tbody>
    </table>
</div>

<script src="~/Scripts/Custom/viewReport.js"></script>