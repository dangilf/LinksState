
@{
    ViewBag.Title = "CreateReport";
}

<h2>CommonReport</h2>

<div id="#app">

</div>

<script>
    const app = new Vue({
        el: '#app',
        data: {
            baseUrl: null,
            nestingLevel: null
        },
        created: function () {
            this.fetchEventsList();
            this.timer = setInterval(this.fetchEventsList, 3000)
        },
        methods: {
            getNewResults: function () {

                this.$http.get('events', function (events) {

                    this.list = events;

                }).bind(this);

            },
            cancelAutoUpdate: function () { clearInterval(this.timer) }

        },
        beforeDestroy() {
            clearInterval(this.timer)
        }
    })
</script>